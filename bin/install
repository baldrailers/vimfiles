echo "\n=> Shougo/unite.vim"
if [ ! -e "$BUNDLE/unite.vim" ]; then git clone git://github.com/Shougo/unite.vim.git "$BUNDLE/unite.vim" && (cd "$BUNDLE/unite.vim" && git checkout -q bc48c2b00d457437585bc1d12f006bdeef7f9a3d 2>/dev/null); fi
