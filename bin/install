#!/usr/bin/env sh
BUNDLE="bundle"
echo "\n=> vim-scripts/ZoomWin"
if [ ! -e "$BUNDLE/ZoomWin" ]; then git clone git://github.com/vim-scripts/ZoomWin.git "$BUNDLE/ZoomWin" && (cd "$BUNDLE/ZoomWin" && git checkout -q cf21ce1f4bfec10fe7e446c08e9e647a2a234e03 2>/dev/null); fi
echo "\n=> editorconfig/editorconfig-vim"
if [ ! -e "$BUNDLE/editorconfig-vim" ]; then git clone git://github.com/editorconfig/editorconfig-vim.git "$BUNDLE/editorconfig-vim" && (cd "$BUNDLE/editorconfig-vim" && git checkout -q v0.3.2 2>/dev/null); fi
